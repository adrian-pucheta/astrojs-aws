{
  "name": "@astrojs-aws/adapter",
  "version": "0.0.0",
  "description": "The Astro adapter of AWS Serverless",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index",
      "require": "./dist/index",
      "types": "./dist/index.d.ts"
    },
    "./exports": {
      "import": "./dist/exports",
      "require": "./dist/exports",
      "types": "./dist/exports.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "aws",
    "aws-lambda",
    "withastro",
    "astro-adapter"
  ],
  "bugs": "https://github.com/helbing/astrojs-aws/issues",
  "homepage": "https://github.com/helbing/astrojs-aws/",
  "repository": {
    "type": "git",
    "url": "https://github.com/helbing/astrojs-aws.git",
    "directory": "packages/constructs"
  },
  "author": "helbing",
  "license": "MIT",
  "engines": {
    "node": "16.x || 18.x"
  },
  "devDependencies": {
    "@middy/core": "^4.2.8",
    "@types/aws-lambda": "^8.10.114",
    "@types/node": "^18.15.11",
    "astro": "^2.1.9",
    "rimraf": "^4.4.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-node-polyfills": "^0.7.0",
    "vitest": "^0.29.8"
  }
}
