{
  "name": "@astrojs-aws/adapter",
  "version": "0.0.0",
  "description": "The astrojs adapter of AWS Lambda",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index",
      "types": "./dist/index.d.ts"
    },
    "./exports": {
      "import": "./dist/exports",
      "types": "./dist/exports.d.ts"
    },
    "./exports.js": {
      "import": "./dist/exports",
      "types": "./dist/exports.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:types": "tsc -p tsconfig.build.json",
    "test": "vitest --run"
  },
  "keywords": [
    "aws",
    "aws-lambda",
    "withastro",
    "astro-adapter"
  ],
  "bugs": "https://github.com/helbing/astrojs-aws/issues",
  "homepage": "https://github.com/helbing/astrojs-aws/",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/helbing/astrojs-aws.git",
    "directory": "packages/constructs"
  },
  "files": [
    "dist"
  ],
  "author": "helbing",
  "license": "MIT",
  "dependencies": {
    "@middy/core": "^4.2.6",
    "@types/node": "^18.15.1",
    "astro": "^2.1.2",
    "aws-lambda": "^1.0.7",
    "http-status-codes": "^2.2.0",
    "node-fetch": "^2.6.9"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.111",
    "@types/babel__traverse": "7.17.1",
    "@types/node-fetch": "^2.6.2",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-node-polyfills": "^0.7.0",
    "vitest": "^0.29.2"
  },
  "engines": {
    "node": ">=16.x"
  }
}
