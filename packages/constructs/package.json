{
  "name": "@astrojs-aws/constructs",
  "version": "0.0.0",
  "description": "The CDK Construct Library of Astro",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "jsii -vvv",
    "test": "vitest run --no-threads",
    "testu": "vitest run --no-threads --update",
    "package": "jsii-pacmak -vvv",
    "clean": "rimraf dist lib/*.js lib.*.d.ts"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "jsii",
    "astro"
  ],
  "bugs": "https://github.com/withastro/astro/issues",
  "homepage": "https://astro-aws.org/",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/helbing/astrojs-aws.git",
    "directory": "packages/constructs"
  },
  "files": [
    "lib"
  ],
  "author": "helbing",
  "license": "MIT",
  "engines": {
    "node": "16.x || 18.x"
  },
  "jsii": {
    "excludeTypescript": [
      "tests",
      "vitest.config.ts",
      "**/*.test.ts"
    ],
    "outdir": "dist",
    "projectReferences": true,
    "targets": {
      "python": {
        "distName": "aws-jsiisamples.jsii-code-samples",
        "module": "aws_jsiisamples.jsii_code_samples"
      }
    }
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.70.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.70.0-alpha.0",
    "@types/node": "18.11.9",
    "aws-cdk-lib": "2.70.0",
    "constructs": "10.1.296",
    "jsii": "^1.79.0",
    "jsii-pacmak": "^1.79.0",
    "rimraf": "^4.4.1",
    "vitest": "^0.29.8"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.70.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.70.0-alpha.0",
    "aws-cdk-lib": "2.70.0",
    "constructs": "10.1.296"
  }
}
