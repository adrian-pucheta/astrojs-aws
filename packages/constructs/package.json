{
  "name": "@astrojs-aws/constructs",
  "version": "0.0.0",
  "description": "The CDK Construct Library of Astro",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "pnpm run clean && jsii -vvv",
    "test": "vitest run --no-threads",
    "testu": "vitest run --no-threads --update",
    "test:integ": "integ-runner --directory tests -l typescript --parallel-regions $CDK_DEPLOY_REGION --update-on-failed -v",
    "package": "jsii-pacmak -vvv",
    "clean": "rimraf dist lib/*.js lib/*.d.ts"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "jsii",
    "astro"
  ],
  "bugs": "https://github.com/withastro/astro/issues",
  "homepage": "https://astro-aws.org/",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/helbing/astrojs-aws.git",
    "directory": "packages/constructs"
  },
  "files": [
    "lib"
  ],
  "author": "helbing",
  "license": "MIT",
  "engines": {
    "node": "16.x || 18.x"
  },
  "jsii": {
    "excludeTypescript": [
      "tests",
      "vitest.config.ts",
      "**/*.test.ts"
    ],
    "outdir": "dist",
    "projectReferences": true,
    "targets": {
      "python": {
        "distName": "aws-jsiisamples.jsii-code-samples",
        "module": "aws_jsiisamples.jsii_code_samples"
      }
    }
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.72.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.72.1-alpha.0",
    "@aws-cdk/integ-runner": "^2.72.1",
    "@aws-cdk/integ-tests-alpha": "2.72.1-alpha.0",
    "@aws-sdk/fetch-http-handler": "^3.303.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/node": "18.11.9",
    "aws-cdk-lib": "2.72.1",
    "aws-lambda": "^1.0.7",
    "cdk-http-pinger": "^0.1.222",
    "constructs": "10.1.301",
    "jsii": "^1.79.0",
    "jsii-pacmak": "^1.79.0",
    "rimraf": "^4.4.1",
    "vitest": "^0.29.8"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.72.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.72.1-alpha.0",
    "aws-cdk-lib": "2.72.1",
    "constructs": "10.1.301"
  }
}
