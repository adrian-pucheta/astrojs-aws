{
  "name": "@astrojs-aws/constructs",
  "version": "0.0.0",
  "description": "The AWS Constructs of AstroSite",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii -vvv",
    "test": "vitest --run",
    "package": "jsii-pacmak -vvv"
  },
  "keywords": [
    "aws",
    "aws-cdk",
    "aws-constructs",
    "astro"
  ],
  "bugs": "https://github.com/withastro/astro/issues",
  "homepage": "https://astro-aws.org/",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/helbing/astrojs-aws.git",
    "directory": "packages/constructs"
  },
  "files": [
    "lib"
  ],
  "author": "helbing",
  "license": "MIT",
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.69.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.69.0-alpha.0",
    "@types/node": "18.11.19",
    "aws-cdk-lib": "^2.69.0",
    "constructs": "^10.1.281",
    "jsii": "^1.78.1",
    "jsii-pacmak": "^1.78.1",
    "vitest": "^0.29.2"
  },
  "engines": {
    "node": ">=16.x"
  },
  "jsii": {
    "excludeTypescript": [
      "examples"
    ],
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "aws-jsiisamples.jsii-code-samples",
        "module": "aws_jsiisamples.jsii_code_samples"
      }
    }
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.69.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.69.0-alpha.0",
    "aws-cdk-lib": "^2.69.0",
    "constructs": "^10.1.281"
  }
}
