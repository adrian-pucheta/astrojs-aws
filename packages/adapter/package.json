{
  "name": "@astrojs-aws/adapter",
  "version": "0.0.0",
  "description": "The Astro adapter of AWS Serverless",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index",
      "require": "./dist/index",
      "types": "./dist/index.d.ts"
    },
    "./exports": {
      "import": "./dist/exports",
      "require": "./dist/exports",
      "types": "./dist/exports.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest run --no-threads"
  },
  "keywords": [
    "aws",
    "aws-lambda",
    "withastro",
    "astro-adapter"
  ],
  "bugs": "https://github.com/helbing/astrojs-aws/issues",
  "homepage": "https://github.com/helbing/astrojs-aws/",
  "repository": {
    "type": "git",
    "url": "https://github.com/helbing/astrojs-aws.git",
    "directory": "packages/constructs"
  },
  "author": "helbing",
  "license": "MIT",
  "engines": {
    "node": "14.x || 16.x || 18.x"
  },
  "dependencies": {
    "@middy/core": "^4.2.6",
    "@types/node": "^18.15.1",
    "astro": "^2.1.2",
    "aws-lambda": "^1.0.7",
    "http-status-codes": "^2.2.0",
    "node-fetch": "^2.6.9"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.111",
    "@types/babel__traverse": "7.17.1",
    "@types/node-fetch": "^2.6.2",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-node-polyfills": "^0.7.0",
    "vitest": "^0.29.2"
  }
}
