{
  "name": "@astrojs-aws/construct",
  "version": "1.0.0",
  "description": "The CDK Construct Library of Astro",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "jsii -vvv",
    "test": "vitest run --no-threads",
    "test:integ": "integ-runner --directory tests --parallel-regions us-east-1 --update-on-failed -v",
    "package": "jsii-pacmak -vvv",
    "publish": "publib --",
    "docgen": "jsii-docgen",
    "clean": "rimraf dist lib/*.js lib/*.d.ts"
  },
  "keywords": [
    "aws",
    "aws-cdk",
    "constructs",
    "jsii",
    "astro"
  ],
  "bugs": "https://github.com/withastro/astro/issues",
  "homepage": "https://astro-aws.org/",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/helbing/astrojs-aws.git",
    "directory": "packages/construct"
  },
  "files": [
    "lib"
  ],
  "author": "helbing",
  "license": "MIT",
  "engines": {
    "node": "16.x || 18.x"
  },
  "stability": "experimental",
  "jsii": {
    "excludeTypescript": [
      "tests",
      "vitest.config.ts",
      "**/*.test.ts"
    ],
    "outdir": "dist",
    "projectReferences": true,
    "metadata": {
      "jsii": {
        "rosetta": {
          "strict": true
        }
      }
    },
    "targets": {
      "python": {
        "distName": "astrojs-aws.construct",
        "module": "astrojs_aws.construct"
      },
      "java": {
        "package": "io.github.helbing.astrojsaws.construct",
        "maven": {
          "groupId": "io.github.helbing.astrojsaws",
          "artifactId": "construct"
        }
      },
      "dotnet": {
        "namespace": "AstrojsAWS.Construct",
        "packageId": "AstrojsAWS.Construct"
      },
      "go": {
        "moduleName": "github.com/helbing/astrojs-aws-go",
        "packageName": "construct"
      }
    }
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.73.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.73.0-alpha.0",
    "@aws-cdk/integ-runner": "^2.73.0-alpha.0",
    "@aws-cdk/integ-tests-alpha": "^2.73.0-alpha.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/node": "18.11.9",
    "aws-cdk-lib": "2.73.0",
    "aws-lambda": "^1.0.7",
    "cdk-http-pinger": "^0.1.222",
    "constructs": "10.1.307",
    "jsii": "^1.80.0",
    "jsii-docgen": "^7.1.46",
    "jsii-pacmak": "^1.80.0",
    "publib": "^0.2.645",
    "rimraf": "^4.4.1",
    "vitest": "^0.29.8"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.73.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.73.0-alpha.0",
    "aws-cdk-lib": "^2.73.0",
    "constructs": "^10.1.307"
  }
}
